<%- include partials/header.ejs %>
<%- include partials/title.ejs %>
<%- include partials/navigation.ejs %>

<!-- Main Content -->
<main class="container">
    <div class="row">
        <div class="col-md-offset-3 col-md-14">
            <h1><%= title %></h1>
            <br />
            <table class="table table-bordered table-striped table-hover">
                <!-- Header Row-->
                <tr>
                    <th class="text-center">ID</th>
                    <th class="text-center">Name</th>
                    <th class="text-center">Description</th>
                </tr>

                <!-- Repeatable Template Row -->

                <% for(let count = 0; count < SurveyList.length; count++) { %>
                    <tr>
                        <td class="text-center"><%= SurveyList[count].id %></td>
                        <td class="text-center"><%= SurveyList[count].name %></td>
                        <td class="text-center"><%= SurveyList[count].Description %></td>
                        
                    </tr>
                <% } %>

            </table>
        </div>
    </div>
</main>

<%- include partials/bottom_nav.ejs %>

<%- include partials/footer.ejs %>